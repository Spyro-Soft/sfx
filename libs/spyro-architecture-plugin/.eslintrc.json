{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {
        "@typescript-eslint/no-var-requires": 0,
        "@typescript-eslint/naming-convention": 1
      }
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.json"
      ],
      "parser": "jsonc-eslint-parser"
    },
    {
      "files": [
        "./package.json",
        "./generators.json"
      ],
      "parser": "jsonc-eslint-parser"
    }
  ]
}
